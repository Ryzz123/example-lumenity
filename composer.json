        "illuminate/validation": "^11.2",
        "illuminate/collections": "^11.2",
        "vlucas/phpdotenv": "^5.6",
        "eftec/bladeone": "^4.13",
        "filp/whoops": "^2.15",
        "symfony/var-dumper": "^7.0",
        "monolog/monolog": "3.5.0",
