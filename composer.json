{
    "name": "lumenity/framework",
    "version": "1.2.0",
    "description": "This is a simple PHP framework for building web applications.",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Lumenity\\Framework\\": "."
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lumenity\\Framework\\": "test"
        }
